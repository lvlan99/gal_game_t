Host 172.110.1.37
  HostName 172.110.1.37
  User ubuntu
  IdentityFile D:/A_self/.ssh/id_ed25519  # 私钥的绝对路径（注意用 / 而非 \）
 UserKnownHostsFile D:/A_self/.ssh/known_hosts  # 强制指定known_hosts路径

